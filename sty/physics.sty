\DeclareDocumentCommand\@matrixquantity{ s g o d() d|| }
{
	\mathord{
		\IfNoValueTF{#2}
		{
			\IfNoValueTF{#3}
			{
				\IfNoValueTF{#4}
				{
					\IfNoValueTF{#5}
					{()}
					{\vmqty{#5}}
				}
				{
					\IfBooleanTF{#1}
					{\Pmqty{#4}}
					{\pmqty{#4}}
					\IfNoValueTF{#5}{}{|#5|}
				}
			}
			{\bmqty{#3} \IfNoValueTF{#4}{}{(#4)} \IfNoValueTF{#5}{}{|#5|}}
		}
		{\begin{matrix}#2\end{matrix} \IfNoValueTF{#3}{}{[#3]} \IfNoValueTF{#4}{}{(#4)} \IfNoValueTF{#5}{}{|#5|}}
	}
	\ifnum\z@=`{\fi}
}
